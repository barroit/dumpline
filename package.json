{
   "name": "dumpline",
   "displayName": "dumpline",
   "description": "Dump selected text to image.",
   "icon": "assets/negi-only.png",
   "type": "module",
   "version": "0.0.1",
   "publisher": "barroit",
   "license": "GPL-3.0-or-later",
   "main": "core/dumpline.js",
   "activationEvents": [],
   "engines": {
      "vscode": "^1.74.0"
   },
   "repository": {
      "type": "git",
      "url": "git@github.com:barroit/dumpline.git"
   },
   "files": [
      "core/*",
      "LICENSES/*",
      "assets/*"
   ],
   "contributes": {
      "commands": [
         {
            "command": "dumpline.exec",
            "title": "Dump line",
            "enablement": "editorHasSelection && !editorHasMultipleSelections"
         }
      ],
      "menus": {
         "editor/context": [
            {
               "command": "dumpline.exec",
               "group": "dumpline",
               "when": "editorHasSelection && !editorHasMultipleSelections"
            }
         ]
      }
   },
   "devDependencies": {
      "@vscode/vsce": "^3.6.0"
   }
}
